{
  "name": "hover-design-prototype",
  "version": "0.0.4",
  "description": "Prototype for Hover Design",
  "license": "MIT",
  "files": [
    "dist/**"
  ],
  "types": "./dist/index.d.ts",
  "main": "./dist/index.umd",
  "typings": "./dist/index.umd",
  "module": "./dist/index.es.js",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js"
    },
    "./styles": {
      "import": "./dist/style.css",
      "require": "./dist/style.css"
    }
  },
  "unpkg": "./dist/index.umd.js",
  "jsdelivr": "./dist/index.umd.js",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "tsc": "tsc",
    "serve": "vite preview",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@vanilla-extract/css": "^1.6.8",
    "@vanilla-extract/recipes": "^0.2.3",
    "@vanilla-extract/sprinkles": "^1.3.3",
    "@vanilla-extract/vite-plugin": "^3.1.0",
    "deepmerge": "^4.2.2",
    "polished": "^4.1.3",
    "react": "^17.0.0",
    "react-dom": "^17.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.16.5",
    "@storybook/addon-actions": "^6.5.0-alpha.4",
    "@storybook/addon-essentials": "^6.5.0-alpha.4",
    "@storybook/addon-links": "^6.5.0-alpha.4",
    "@storybook/react": "^6.5.0-alpha.4",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@vitejs/plugin-react": "^1.0.0",
    "babel-loader": "^8.2.3",
    "storybook-builder-vite": "^0.1.11",
    "typescript": "^4.3.2",
    "vite": "^2.6.4",
    "vite-plugin-dts": "^0.9.9",
    "vite-tsconfig-paths": "^3.4.0"
  },
  "peerDependencies": {
    "react": "^17",
    "react-dom": "^17"
  }
